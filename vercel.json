{
  "version": 2,
  "builds": [
    { "src": "api/wsgi.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/wsgi.py" },
    { "src": "/(.*)", "dest": "static/$1" }
  ],
  "installCommand": "pip install -r api/requirements.txt",
  "buildCommand": "python -c \"print(\'Build complete\')\""
}
