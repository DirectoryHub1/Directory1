{
  "version": 2,
  "builds": [
    {
      "src": "api/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.9.18"
      }
    },
    { "src": "static/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/wsgi.py" },
    { "src": "/(.*)", "dest": "static/$1" }
  ],
  "installCommand": "pip install -r api/requirements.txt",
  "buildCommand": "python -c \"print('Build complete')\"",
  "env": {
    "PYTHONPATH": "/var/task/api"
  }
}



